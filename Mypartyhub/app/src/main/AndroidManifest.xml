<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team12.wfc.mypartyhub" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Acitivity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Acitivity.postactivity"
            android:label="@string/title_activity_postactivity" >
        </activity>
        <activity
            android:name=".Acitivity.Fotogalery"
            android:label="@string/title_activity_fotogalery" >
        </activity>
        <activity
            android:name=".Acitivity.main_menu"
            android:label="@string/title_activity_main_menu" >
        </activity>
    </application>

</manifest>
